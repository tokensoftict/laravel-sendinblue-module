{
  "name": "tokensoft/tokensoft-laravel-sendinblue",
  "description": "Laravel's mail transport for SendinBlue",
  "keywords": ["laravel", "laravel5", "lumen", "sendinblue", "stmp", "mailing"],
  "license": "MIT",
  "authors": [
    {
      "name": "TokenSoft ICT",
      "homepage": "https://github.com/tokensoft",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/mail": "~5",
    "sendinblue/api-v3-sdk": "~8.0"
  },

  "autoload": {
    "psr-4": {
      "TokenSoft\\LaravelSendinBlue\\": "src/"
    }
  },

  "extra": {
    "laravel": {
      "providers": [
        "TokenSoft\\LaravelSendinBlue\\SendinBlueServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
